{
  "env": {
    "es6": true,
    "browser": true,
    "node": true,
    "meteor": true
  },

  "extends": "airbnb",

  "parser": "babel-eslint",

  "settings": {
    "import/resolver": {
      "meteor": {
        "extensions": [
          ".js",
          ".jsx"
        ]
      }
    }
  },

  "rules": {
    "no-console": "off"
  }
}
